          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="VWBloqueoTableAdapter" GeneratorDataComponentClassName="VWBloqueoTableAdapter" Name="VWBloqueo" UserDataComponentName="VWBloqueoTableAdapter">
            <MainSource>
