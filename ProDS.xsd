                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Vw_Anexos.AnexoCon, Vw_Anexos.Descr AS Cliente, { fn UCASE(GEN_Bloqueo_Tasas.ComentarioPromo) } AS ComentarioPromo, CASE WHEN aNEXOS.Tipta = '7' THEN CONVERT(varchar, Tasapol) 
                         ELSE 'TIIE + ' + CONVERT(varchar, Tasapol) END AS Tasa_Politica, CASE WHEN aNEXOS.Tipta = '7' THEN CONVERT(varchar, TasaSol) ELSE 'TIIE + ' + CONVERT(varchar, TasaSol) END AS Tasa_Solicitada, 
                         Vw_Anexos.TipoCredito, Vw_Anexos.MontoFinanciado, Vw_Anexos.FondoReserva, Anexos.Plazo, CASE WHEN aNEXOS.Tipta = '7' THEN 'FIJA' ELSE 'VARIABLE' END AS Tipo_Tasa, 
                         Anexos.Porco AS PorcentajeComision, Anexos.Porop AS RecidualOP, Anexos.Taspen AS Penalizacion, Anexos.RD AS RentasenDeposito, Anexos.DG AS DespoitoenGarantia, GEN_Bloqueo_Tasas.id, 
                         GEN_Bloqueo_Tasas.TasaPol - GEN_Bloqueo_Tasas.TasaSol AS Diferencia, GEN_Bloqueo_Tasas.ComentarioRiesgos, GEN_Bloqueo_Tasas.AutorizadoRI, GEN_Bloqueo_Tasas.AutorizadoDG, 
                         GEN_Bloqueo_Tasas.FirmaSubPromo
FROM            GEN_Bloqueo_Tasas INNER JOIN
                         Vw_Anexos ON SUBSTRING(GEN_Bloqueo_Tasas.Anexo, 2, 9) = SUBSTRING(Vw_Anexos.Anexo, 2, 9) INNER JOIN
                         Anexos ON SUBSTRING(GEN_Bloqueo_Tasas.Anexo, 2, 9) = SUBSTRING(Anexos.Anexo, 2, 9)
WHERE        (GEN_Bloqueo_Tasas.id = @ID)</CommandText>
